                  <div class="x_title">
                    <h2> <small>Diligencie el formulario</small></h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                      <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>                        
                      </li>
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">

                    <form class="form-horizontal form-label-left" id="formulario" name="formulario" action="" method="post" data-toggle="validate">

                      <p>Ingrese los datos que solicitan para crear tipo de asignaci贸n
                      </p>
                      <span class="section">Formulario</span>

                      <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">Nombre <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <input id="name" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" data-validate-words="1" name="nombre"  placeholder="Nombre asignaci贸n" required="required" type="text">
                        </div>
                      </div>
                       <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="textarea">Descripci贸n <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <textarea id="textarea" name="desc" class="form-control col-md-7 col-xs-12" id="desc" required="required"></textarea>
                        </div>
                      </div>
                      
                      <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="textarea">Estado <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          <select id="estado" name="estado" class="form-control col-md-7 col-xs-12" required="required">
                              <option value="1" >Activo</option>
                              <option value="0" >Inactivo</option>
                          </select>
                        </div>
                      </div>
                     
                    <div class="separator">
                        <p class="change_link">
                        <h5> {% if guardado is defined %} Guardado {% endif %}</h5>
                        </p>
                    </div>
                      
                      <div class="ln_solid"></div>
                      <div class="form-group">
                        <div class="col-md-6 col-md-offset-3">
                            <button id="send" id="send" type="submit" class="btn btn-round btn-success">Enviar</button> 
                            <button id="cancel" name="cancel" type="reset" class="btn btn-round btn-danger">Cancel</button>
                            <button id="lista" name="lista" type="button" class="btn btn-round btn-primary">Lista</button>
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>
        {% include '@Tipoasignacion/Default/includes/tabla.html.twig' %}
